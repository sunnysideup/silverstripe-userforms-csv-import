{
  "name": "sunnysideup/userforms-csv-import",
  "description": "Allows you to create records for a userdefined form using a csv (e.g. export from another db instance)",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "csv",
    "userforms",
    "userdefinedforms",
    "data management",
    "import",
    "csv import",
    "migration"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*",
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7",
    "silverstripe/standards": "^1",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\UserformsCsvImport\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/npm-build-userforms-csv-import.sh",
    "bin/npm-watch-userforms-csv-import.sh"
  ],
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ]
}
